hText: '',\n    }\n  },\n  methods: {\n    RemoveProduct(product) {\n      this.$root.$data.cart.splice(this.$root.$data.cart.indexOf(product), 1);\n    }\n  }\n}\n</script>\n\n<style scoped>\n.wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n.search {\n  border: 1px solid #ccc !important;\n  border-radius: 4px;\n  width: 50%;\n}\n.name {\n  font-size: 25px;\n}\nbutton {\n  /* background-color: #DFBBB6; */\n  padding: 5px;\n  font-size: 20px;\n}\nform {\n  display: table;\n  width: 100%;\n}\ni {\n  display: table-cell;\n  padding-left: 10px;\n  width: 1px;\n}\ninput {\n  display: table-cell;\n  font-size: 20px;\n  border: none !important;\n  box-shadow: none !important;\n  width: 100%;\n  height: 40px;\n}\n.ifStatement {\n  text-align: center;\n}\nimg {\n  height: 300px;\n  width: 300px;\n  object-fit: cover;\n}\n.ElseStatment {\n  display: flex;\n  justify-content: center;\n  flex-wrap: wrap;\n}\n\n.productBox{\n  border: round;\n  background-color: #ffffff;\n  padding: 5px;\n}\n</style>\n"]}]}